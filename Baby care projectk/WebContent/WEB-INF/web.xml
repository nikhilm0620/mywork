<web-app xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
<servlet>
<servlet-name>AdminLoginServlet</servlet-name>
<servlet-class>com.nk.admin.AdminLoginServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AdminLoginServlet</servlet-name>
<url-pattern>/log</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>AddBabyNamesServlet</servlet-name>
<servlet-class>com.nk.admin.AddBabyNamesServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AddBabyNamesServlet</servlet-name>
<url-pattern>/AddBabyName</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>AddBabyProductsServlet</servlet-name>
<servlet-class>com.nk.admin.AddBabyProductsServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AddBabyProductsServlet</servlet-name>
<url-pattern>/AddBabyProduct</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>AddBabyFoodsServlet</servlet-name>
<servlet-class>com.nk.admin.AddBabyFoodsServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AddBabyFoodsServlet</servlet-name>
<url-pattern>/AddBabyFood</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>UserRegistrationServlet</servlet-name>
<servlet-class>com.nk.user.UserRegistrationServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>UserRegistrationServlet</servlet-name>
<url-pattern>/reguser</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>UserLoginServlet</servlet-name>
<servlet-class>com.nk.user.UserLoginServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>UserLoginServlet</servlet-name>
<url-pattern>/loguser</url-pattern>
</servlet-mapping>
<welcome-file-list>
<welcome-file>index.html</welcome-file>
</welcome-file-list>
</web-app>